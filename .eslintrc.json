{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "prettier/react"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {},
  "prettier/prettier": [
    "error",
    {
      "trailingComma": "es5",
      "singleQuote": true,
      "printWidth": 120
    }
  ],
  "plugins": [
    //html
    "prettier"
  ]
}
